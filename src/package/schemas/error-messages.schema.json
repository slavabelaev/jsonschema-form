    "minProperties": {
      "type": "string",
      "default": "должно иметь не менее, чем ${limit} ${declension(limit, ['поле', 'поля', 'полей'])}"
    },
    "multipleOf": {
      "type": "string",
      "default": "должно быть кратным ${multipleOf}"
    },
    "not": {
      "type": "string",
      "default": "должно не соответствовать схеме в \"not\""
    },
    "oneOf": {
      "type": "string",
      "default": "должно соответствовать в точности одной схеме в \"oneOf\""
