    },
    "switch": {
      "type": "string",
      "default": "должно соответствовать правилу ${caseIndex} в \"switch\""
    },
    "type": {
      "type": "string",
      "default": "должно быть ${type}"
    },
    "uniqueItems": {
      "type": "string",
      "default": "не должно иметь повторяющихся элементов (элементы ${j} и ${i} идентичны)"
    }
  }
