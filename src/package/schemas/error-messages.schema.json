    "const": {
      "type": "string",
      "default": "должно быть равно заданному значению"
    },
    "contains": {
      "type": "string",
      "default": "должно содержать значение соответствующее схеме"
    },
    "custom": {
      "type": "string",
      "default": "должно соответствовать правилу ${keyword}"
    },
    "dependencies": {
      "type": "string",
      "default": "должно иметь поля ${deps}, когда присутствует поле ${property}"
