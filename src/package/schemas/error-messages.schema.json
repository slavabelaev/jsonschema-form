      "default": "${keyword} должно быть boolean"
    },
    "formatExclusiveMinimum": {
      "type": "string",
      "default": "${keyword} должно быть boolean"
    },
    "formatMaximum": {
      "type": "string",
      "default": "должно быть ${comparison} ${limit}"
    },
    "formatMinimum": {
      "type": "string",
      "default": "должно быть ${comparison} ${limit}"
    },
    "if": {
